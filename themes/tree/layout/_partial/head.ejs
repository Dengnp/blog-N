<head>
	<meta http-equiv="content-type" content="text/html; charset=utf-8">
	<meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport">
	
	<!-- title -->
	<% 
		var title = page.title; 
		if ( title == '' ) {
			var title = page.slug.split("/")[page.slug.split("/").length-1]
		}else {
			var title = page.title; 
		};
	%>
	<title>
	<% if (title){ %>
		<%= title %> | 
	<% } %> 
	<%= config.title %>
	</title>
	
	<!-- keywords,description -->
	<% if (page.keywords){ %>
		<meta name="keywords" content="<%= page.keywords %>,<%= config.keywords %>" />
	<% } else if (config.keywords){ %>
		<meta name="keywords" content="<%= config.keywords %>" />
	<% } %> <% if (page.description){ %>
		<meta name="description" content="<%= page.description %>" />
	<% } else if (config.description){ %>
		<meta name="description" content="<%= config.description %>" />
	<% } %>

	<!-- favicon -->
	<% if (theme.favicon){ %>
	<%- favicon_tag(theme.favicon) %>
	<% } %>


	<!-- search -->
	<script>
		var searchEngine = "<%- theme.searchEngine %>";
		if(typeof searchEngine == "undefined" || searchEngine == null || searchEngine == ""){
			searchEngine = "https://www.google.com/search?q=";
		}
		var homeHost = "<%- theme.homeHost %>";
		if(typeof homeHost == "undefined" || homeHost == null || homeHost == ""){
			homeHost = window.location.host;
		}
	</script>


	<%- css('css/main.css') %>
	<%- css('lib/font-awesome/css/font-awesome.min.css') %>
	<%- css('lib/highlight/darcula.css') %>
	<%- css('lib/fancybox/jquery.fancybox.min.css') %>
	<%- css('lib/mCustomScrollbar/jquery.mCustomScrollbar.css') %>


	<%- js('lib/jquery/jquery.min.js') %>
	<%- js('lib/fancybox/jquery.fancybox.min.js') %>
	<%- js('lib/highlight/highlight.min.js') %>
	<%- js('lib/jquery/jquery.pjax.js') %>
	<%- js('lib/mCustomScrollbar/jquery-ui-1.10.4.min') %>
	<%- js('lib/mCustomScrollbar/jquery.mousewheel.min.js') %>
	<%- js('lib/mCustomScrollbar/jquery.mCustomScrollbar.js') %>
	<%- js('js/main.js') %>
	<% if (theme.valine){ %>
		<%- js('lib/valine/av-min.js') %>
		<%- js('lib/valine/Valine-1.3.10-min.js') %>
	<% } %>
	<% if (theme.busuanzi){ %>
		<script async src="/lib/busuanzi/busuanzi.pure.mini.js"></script>
	<% } %>
</head>